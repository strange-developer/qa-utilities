{
  "name": "test-utilities",
  "version": "0.0.1",
  "description": "Test utilities",
  "main": "./lib/index.js",
  "scripts": {
    "test": "npm run lint && jest --coverage",
    "test:coveralls": "npm run lint && jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:scripts": "jest --coverage",
    "test:vulnerabilities": "npm audit --parseable",
    "lint": "eslint ./src && eslint ./test",
    "prepublishOnly": "babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strange-developer/test-utilities.git"
  },
  "author": "Chamir Bodasing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strange-developer/test-utilities/issues"
  },
  "homepage": "https://github.com/strange-developer/test-utilities#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^23.5.0"
  }
